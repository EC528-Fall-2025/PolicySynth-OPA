{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "DenyAllEC2AndRDSOperations",
      "Effect": "Deny",
      "Action": [
        "ec2:*",
        "rds:*"
      ],
      "Resource": "*"
    },
    {
      "Sid": "DenyLambdaInSpecificRegions",
      "Effect": "Deny",
      "Action": "lambda:*",
      "Resource": "arn:aws:lambda:ap-*:*:*"
    }
  ]
}
